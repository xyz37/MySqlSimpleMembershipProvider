		</authentication>

		<membership defaultProvider="MySqlSimpleMembershipProvider">
			<providers>
				<clear />
				<add name="MySqlSimpleMembershipProvider"
					 type="MySql.Web.Security.MySqlSimpleMembershipProvider, MySql.Web.Extension" />
